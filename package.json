{
  "name": "algo-lens",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/blackphoenix42/AlgoLens.git"
  },
  "engines": {
    "node": ">=22.18.0 <23"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "changeset": "changeset",
    "release:version": "changeset version",
    "release:publish": "changeset publish",
    "prepare": "husky",
    "typecheck": "tsc --noEmit",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --max-warnings=0",
    "lint:scripts": "eslint scripts/ --max-warnings=0",
    "test": "vitest ",
    "test:e2e": "playwright test --reporter=dot",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "pwsh -NoProfile -Command \"$env:PWDEBUG='1'; playwright test\"",
    "e2e:report": "playwright show-report",
    "test:e2e:dev": "cross-env PW_BASE_URL=http://127.0.0.1:5173 playwright test",
    "test:a11y": "pa11y-ci e2e/pa11.json",
    "test:percy": "percy exec -- playwright test",
    "analyze:bundle": "node e2e/analyze.mjs --dir=dist --out=analysis --budget-js=250kb --budget-total=600kb --fail-on-threshold",
    "lhci:server": "http-server ./dist -p 4173 -s",
    "lhci:collect": "lhci collect --config=lhci/lighthouserc.json",
    "lhci:assert": "lhci assert --config=lhci/lighthouserc.json",
    "lhci:upload": "lhci upload --config=lhci/lighthouserc.json",
    "lhci": "npm run build && lhci autorun --config=lhci/lighthouserc.json",
    "a11y": "pa11y-ci -c pa11y-ci.json",
    "a11y:report": "pa11y-ci -c pa11y-ci.json --json > reports/a11y/report.json",
    "a11y:ci": "npm run build && start-server-and-test \"vite preview --port 4173 --strictPort\" http://127.0.0.1:4173 \"npm run a11y\"",
    "postbuild": "node scripts/postbuild.mjs",
    "licenses": "node scripts/check-licenses.mjs",
    "sitemap": "node scripts/generate-sitemap.mjs --base https://algolens.app --out public/sitemap.xml",
    "perf": "node scripts/perf-benchmark.mjs --url http://127.0.0.1:4173 --tries 3",
    "adr": "node scripts/adr-new.mjs",
    "dev:utils": "node scripts/dev-utils.mjs",
    "clean": "node scripts/dev-utils.mjs clean",
    "reset": "node scripts/dev-utils.mjs reset",
    "logging:setup": "node scripts/setup-logging.mjs",
    "logging:debug": "node scripts/setup-logging.mjs debugging",
    "logging:performance": "node scripts/setup-logging.mjs performance",
    "logging:production": "node scripts/setup-logging.mjs production",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@ffmpeg/ffmpeg": "^0.12.15",
    "@ffmpeg/util": "^0.12.2",
    "@sentry/react": "10.10.0",
    "@sentry/replay": "^7.120.4",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "gif-encoder-2": "^1.0.5",
    "gif.js": "0.2.0",
    "highlight.js": "^11.11.1",
    "html2canvas": "1.4.1",
    "i18next": "25.5.1",
    "i18next-browser-languagedetector": "8.2.0",
    "jspdf": "3.0.2",
    "katex": "^0.16.22",
    "lucide-react": "0.542.0",
    "posthog-js": "1.261.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-i18next": "15.7.3",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^7.8.2",
    "react-toastify": "11.0.5",
    "recharts": "3.1.2",
    "rehype-katex": "^7.0.1",
    "remark-gfm": "^4.0.1",
    "remark-math": "^6.0.0",
    "web-vitals": "4.2.4"
  },
  "devDependencies": {
    "@changesets/cli": "2.29.6",
    "@chromatic-com/storybook": "4.1.1",
    "@commitlint/cli": "19.8.1",
    "@commitlint/config-conventional": "19.8.1",
    "@lhci/cli": "0.15.1",
    "@playwright/test": "1.55.0",
    "@sentry/vite-plugin": "4.3.0",
    "@storybook/addon-a11y": "9.1.4",
    "@storybook/addon-docs": "9.1.4",
    "@storybook/addon-onboarding": "9.1.4",
    "@storybook/addon-vitest": "9.1.4",
    "@storybook/react-vite": "9.1.4",
    "@tailwindcss/postcss": "4.1.12",
    "@tailwindcss/vite": "4.1.13",
    "@testing-library/jest-dom": "6.8.0",
    "@testing-library/react": "16.3.0",
    "@testing-library/user-event": "14.6.1",
    "@types/gif.js": "0.2.5",
    "@types/node": "24.3.1",
    "@types/react": "^18.3.4",
    "@types/react-dom": "^18.3.0",
    "@types/testing-library__jest-dom": "6.0.0",
    "@typescript-eslint/eslint-plugin": "^8.41.0",
    "@typescript-eslint/parser": "^8.42.0",
    "@vitejs/plugin-react": "^5.0.2",
    "@vitest/browser": "3.2.4",
    "@vitest/coverage-v8": "3.2.4",
    "autoprefixer": "^10.4.20",
    "buffer": "6.0.3",
    "eslint": "9.35.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-playwright": "2.2.2",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "eslint-plugin-storybook": "9.1.4",
    "eslint-plugin-unused-imports": "4.2.0",
    "events": "3.3.0",
    "http-server": "14.1.1",
    "husky": "9.1.7",
    "jsdom": "26.1.0",
    "lint-staged": "16.1.6",
    "pa11y-ci": "4.0.1",
    "playwright": "^1.55.0",
    "postcss": "^8.4.41",
    "prettier": "^3.3.3",
    "start-server-and-test": "2.1.0",
    "storybook": "9.1.4",
    "stream-browserify": "3.0.0",
    "tailwindcss": "^4.1.12",
    "typescript": "^5.5.4",
    "typescript-eslint": "8.41.0",
    "util": "0.12.5",
    "vite": "^7.1.4",
    "vitest": "3.2.4"
  }
}
