{
  "name": "algo-lens",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/blackphoenix42/AlgoLens.git"
  },
  "engines": {
    "node": ">=22.18.0 <23"
  },
  "scripts": {
    "start": "vite",
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "changeset": "changeset",
    "release:version": "changeset version",
    "release:publish": "changeset publish",
    "prepare": "husky",
    "typecheck": "tsc --noEmit",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --max-warnings=0",
    "lint:scripts": "eslint scripts/ --max-warnings=0",
    "lint:css": "stylelint \"src/**/*.css\"",
    "lint:css:fix": "stylelint \"src/**/*.css\" --fix",
    "lint:md": "markdownlint-cli2 \"**/*.md\" \"#node_modules\" \"#.npm-cache\"",
    "lint:md:fix": "markdownlint-cli2 --fix \"**/*.md\" \"#node_modules\" \"#.npm-cache\"",
    "lint:all": "npm run lint && npm run lint:css && npm run lint:md && npm run typecheck",
    "lint:fix": "npm run lint:css:fix && npm run lint:md:fix && eslint . --fix",
    "check:all": "npm run format:check && npm run lint:all && npm run test --run && echo --- Dependency Report --- && (npm run deps:unused || echo Dependency check completed) && echo --- License Report --- && (npm run licenses:check || echo License check completed)",
    "check:all:fix": "npm run format && npm run lint:fix && npm run typecheck && npm run test --run && echo --- Dependency Report --- && (npm run deps:unused || echo Dependency check completed) && echo --- License Report --- && (npm run licenses:check || echo License check completed)",
    "pre-commit:checks": "npm run format:check && npm run lint:all && npm run typecheck && npm run test --run",
    "pre-push:checks": "npm run check:all && npm run analyze:bundle",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "vitest ",
    "test:e2e": "playwright test --reporter=dot",
    "test:e2e:ci": "cross-env GITHUB_ACTIONS=true GITHUB_REPOSITORY=blackphoenix42/algolens-private npm run build && start-server-and-test \"vite preview --port 4173 --strictPort --host 127.0.0.1\" \"http-get://127.0.0.1:4173/algolens-private/\" \"playwright test --reporter=dot\"",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "pwsh -NoProfile -Command \"$env:PWDEBUG='1'; playwright test\"",
    "e2e:report": "playwright show-report",
    "test:e2e:dev": "cross-env PW_BASE_URL=http://127.0.0.1:5173 playwright test",
    "test:a11y": "pa11y-ci e2e/pa11.json",
    "test:percy": "percy exec -- playwright test",
    "analyze:bundle": "node e2e/analyze.mjs --dir=dist --out=analysis --budget-js=250kb --budget-total=600kb --fail-on-threshold",
    "analyze:visualizer": "cross-env ANALYZE=true npm run build",
    "deps:check": "depcheck",
    "deps:unused": "depcheck --ignores=\"@types/*,@typescript-eslint/*,eslint-*,prettier-*,@testing-library/*,@storybook/*,@playwright/*,vitest,jsdom\" --ignore-patterns=\"dist/**,coverage/**,storybook-static/**\"",
    "licenses:check": "license-checker --summary",
    "licenses:detailed": "license-checker --csv --out licenses.csv",
    "lhci:server": "http-server ./dist -p 4173 -s",
    "lhci:collect": "lhci collect --config=lhci/lighthouserc.json",
    "lhci:assert": "lhci assert --config=lhci/lighthouserc.json",
    "lhci:upload": "lhci upload --config=lhci/lighthouserc.json",
    "lhci": "npm run build && lhci autorun --config=lhci/lighthouserc.json",
    "a11y": "pa11y-ci -c pa11y-ci.json",
    "a11y:report": "pa11y-ci -c pa11y-ci.json --json > reports/a11y/report.json",
    "a11y:ci": "cross-env GITHUB_ACTIONS=true GITHUB_REPOSITORY=blackphoenix42/algolens-private npm run build && start-server-and-test \"vite preview --port 4173 --strictPort --host 127.0.0.1\" \"http-get://127.0.0.1:4173/algolens-private/\" \"npm run a11y\"",
    "postbuild": "node scripts/postbuild.mjs",
    "licenses": "node scripts/check-licenses.mjs",
    "sitemap": "node scripts/generate-sitemap.mjs --base https://algolens.app --out public/sitemap.xml",
    "perf": "node scripts/perf-benchmark.mjs --url http://127.0.0.1:4173 --tries 3",
    "adr": "node scripts/adr-new.mjs",
    "dev:utils": "node scripts/dev-utils.mjs",
    "clean": "node scripts/dev-utils.mjs clean",
    "reset": "node scripts/dev-utils.mjs reset",
    "logging:setup": "node scripts/setup-logging.mjs",
    "logging:debug": "node scripts/setup-logging.mjs debugging",
    "logging:performance": "node scripts/setup-logging.mjs performance",
    "logging:production": "node scripts/setup-logging.mjs production",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@ffmpeg/ffmpeg": "^0.12.15",
    "@ffmpeg/util": "^0.12.2",
    "@react-spring/web": "10.0.2",
