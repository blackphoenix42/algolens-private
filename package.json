    "gif-encoder-2": "^1.0.5",
    "gif.js": "0.2.0",
    "highlight.js": "^11.11.1",
    "html2canvas": "1.4.1",
    "i18next": "25.5.2",
    "i18next-browser-languagedetector": "8.2.0",
    "javascript-algorithms": "0.0.5",
    "jspdf": "3.0.2",
    "katex": "^0.16.22",
    "konva": "10.0.2",
    "lucide-react": "0.544.0",
    "openai": "5.22.0",
    "posthog-js": "1.266.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-i18next": "15.7.3",
    "react-is": "19.1.1",
    "react-konva": "19.0.10",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^7.9.1",
    "react-toastify": "11.0.5",
    "reactflow": "11.11.4",
    "recharts": "3.2.1",
    "rehype-katex": "^7.0.1",
    "remark-gfm": "^4.0.1",
    "remark-math": "^6.0.0",
    "socket.io-client": "4.8.1",
    "web-vitals": "5.1.0",
    "zod": "3.25.76"
